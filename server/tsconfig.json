{
  "compilerOptions": {
    "target": "ES2022",                          
    "module": "commonjs",
    "moduleResolution":"Node",                  
    "lib": ["ES2023"],                     
    "allowJs": false,
    "outDir": "dist",                          
    "rootDir": "src",
    "noUnusedParameters": false,
    "strict": true,         
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strictPropertyInitialization": false,
    "types": [
      "node", "express"
    ]
  }
}